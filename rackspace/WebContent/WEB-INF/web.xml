<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>rackspace</display-name>
  <welcome-file-list>
    <welcome-file>/WEB-INF/JSP/Login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>login.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MyRegister</servlet-name>
    <jsp-file>/WEB-INF/JSP/Registration.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyRegister</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Register</servlet-name>
    <jsp-file>/WEB-INF/JSP/Registration.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>registration.Reg</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/registration1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Home</servlet-name>
    <jsp-file>/WEB-INF/JSP/Home.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/Home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>database</servlet-name>
    <servlet-class>DAO.RegServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>database</servlet-name>
    <url-pattern>/data</url-pattern>
  </servlet-mapping>
  <filter>
  <filter-name>filter</filter-name>
  <filter-class>filter.Loginfliter</filter-class>
  </filter>
  <filter-mapping>
  <filter-name>filter </filter-name>
  <url-pattern>/LoginServlet</url-pattern>
 </filter-mapping>
</web-app>